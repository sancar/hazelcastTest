<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config hazelcast-config-3.6.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <properties>
        <!--<property name="hazelcast.socket.receive.buffer.size">2048</property>-->
        <!--<property name="hazelcast.socket.send.buffer.size">2048</property>-->
        <!--<property name="hazelcast.partition.max.parallel.replications">1</property>-->
        <property name="hazelcast.clientengine.thread.count">1000</property>
        <property name="hazelcast.partition.count">5000</property>
        <property name="hazelcast.logging.type">log4j</property>
        <!--<property name="hazelcast.health.monitoring.level">NOISY</property>-->
        <!--<property name="hazelcast.health.monitoring.delay.seconds">30</property>-->
        <!--<property name="hazelcast.performance.metric.level">INFO</property>-->
        <!--<property name="hazelcast.performance.monitoring.enabled">true</property>-->
        <!--<property name="hazelcast.slow.operation.detector.enabled">true</property>-->
        <!--<property name="hazelcast.slow.operation.detector.stacktrace.logging.enabled">true</property>-->
        <!--<property name="hazelcast.slow.invocation.detector.threshold.millis">10000</property>-->
    </properties>
    <!--<management-center enabled="true">http://nkp-hmgmt-1001.apple.com:8080/mancenter</management-center>-->
    <license-key>
        KEYHERE
    </license-key>

    <network>
        <port auto-increment="true">5701</port>
        <join>
            <multicast enabled="false">
                <multicast-group>224.2.2.3</multicast-group>
                <multicast-port>54327</multicast-port>
            </multicast>
            <tcp-ip enabled="true">
                <interface>127.0.0.1</interface>
            </tcp-ip>
        </join>
    </network>

    <cache name="AOS*">
        <in-memory-format>NATIVE</in-memory-format>
        <backup-count>0</backup-count>
        <eviction size="25" eviction-policy="LRU" max-size-policy="FREE_NATIVE_MEMORY_PERCENTAGE"/>
        <statistics-enabled>true</statistics-enabled>
    </cache>
    <cache name="2AOS*">
        <backup-count>1</backup-count>
        <async-backup-count>1</async-backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction size="25" eviction-policy="LRU" max-size-policy="FREE_NATIVE_MEMORY_PERCENTAGE"/>
        <statistics-enabled>true</statistics-enabled>
    </cache>

    <native-memory enabled="true" allocator-type="POOLED">
        <size value="20" unit="GIGABYTES"/>
    </native-memory>


</hazelcast>
